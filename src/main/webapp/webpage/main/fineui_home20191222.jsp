<script>
    $('select').comboSelect();
    ProgressBar();
    var H1=$('#swiper-table').height();
    H2=H1-80;
    $('#table-java-base1').bootstrapTable({
        cache : false,
        striped : true,
        height:H2,
        pageNumber : 1,// 分页的时候设置当前的页码。
        columns:[
            {
                field:'bh',
                title:'监测点',
                valign:"middle",
                align:"center",
                formatter:function(value,row,index) {
                    return '<span class="color-a0c">'+value+'</span>'
                }
            },
            {
                field:'bh',
                title:'时间',
                valign:"middle",
                align:"center",
                formatter:function(value,row,index) {
                    return '<span class="color-5e7">'+value+'</span>'
                }
            },
            {
                field:'bh',
                title:'参数',
                valign:"middle",
                align:"center",
                formatter:function(value,row,index) {
                    return '<span class="color-a0c">'+value+'</span>'
                }
            },
            {
                field:'bh',
                title:'值',
                valign:"middle",
                align:"center",
                formatter:function(value,row,index) {
                    return '<span class="color-ef8">'+index+'</span>'
                }
            }
        ],
        onLoadSuccess:function(){
            $(".table-side .fixed-table-body").mCustomScrollbar({
                scrollInertia:150
            });
            var MyMar1=setInterval(Marquee1, 30);
            $('#swiper-table .mCSB_container').on('mouseover',function () {
                clearInterval(MyMar1)
            })
            $('#swiper-table .mCSB_container').on('mouseout',function () {
                MyMar1 = setInterval(Marquee1, 30)
            })

        },
        pageSize: 20,
        pageList: [20],
        url: "fg/data/gd.json"
    });
    function Marquee1() {
        var height1=$('#swiper-table .mCSB_container').height();
        var height2=$('.table-side .fixed-table-container').height();
        var height=height1-height2;
        var top=$('#swiper-table .mCSB_container').css('top');
        var top2=$('#swiper-table .mCSB_dragger').css('top');
        var index=top.indexOf('px');
        var inde2x=top2.indexOf('px');
        top=top.substring(0,index);
        top2=top2.substring(0,index);
        if(Math.abs(top)<height){
            top--;
            top2=((Math.abs(top)/height)*(height2-30));
        }else{
            top=0;
            top2=0;
        }
        $('#swiper-table .mCSB_container').css('top',top);
        $('#swiper-table .mCSB_dragger').css('top',top2);
    }




</script>
